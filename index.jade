doctype html
head
  meta(charset='utf-8')
  title
body
  p
  script.
    var char_map = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789';
    var result = '';

    for (var i = 0; i < 16; i++)
      result += char_map.charAt(Math.floor(Math.random() * char_map.length));

    console.log(result);
    document.title = result;
    document.getElementsByTagName('p')[0].innerHTML = result;
